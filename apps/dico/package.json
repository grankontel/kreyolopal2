{
  "name": "dico",
  "version": "0.1.0",
  "private": true,
  "scripts": {
    "dev": "next dev -p 3000",
    "build": "next build",
    "start": "next start",
    "lint": "next lint",
    "pages:build": "npx @cloudflare/next-on-pages",
    "preview": "npm run pages:build && wrangler pages dev .vercel/output/static",
    "deploy": "npm run pages:build && wrangler pages deploy .vercel/output/static --project-name dico",
    "deploy:prod": "NODE_ENV='production' npm run pages:build && wrangler pages deploy .vercel/output/static --project-name dico --branch=develop"
  },
  "dependencies": {
    "bulma": "^0.9.4",
    "classnames": "^2.5.1",
    "feather-icons": "^4.29.0",
    "next": "14.1.0",
    "react": "^18",
    "react-bulma-components": "^4.1.0",
    "react-dom": "^18"
  },
  "devDependencies": {
    "@cloudflare/next-on-pages": "^1.9.0",
    "@cloudflare/workers-types": "^4.20240208.0",
    "@creativebulma/bulma-tooltip": "^1.2.0",
    "@kreyolopal/web-ui": "*",
    "eslint": "^8",
    "eslint-config-next": "14.1.0",
    "eslint-plugin-next-on-pages": "^1.9.0",
    "sass": "^1.58.3",
    "vercel": "^33.5.1",
    "wrangler": "^3.28.3"
  }
}
